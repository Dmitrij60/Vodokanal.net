{% extends template %}

{% block title %} Добавить запись {% endblock %}

{% block body %}
    <div class="starter-template">
        <h1>Добавить запись:</h1>
        <hr>
        <br>

      {{ form_start(orderForm) }}


        <table class="table table-hover">
            <thead>
            <tr>
                <th scope="col">Email</th>
                <th scope="col">Пароль</th>
                <th scope="col">Комментарий</th>


            </tr>
            </thead>
            <tbody>
            <tr>
                <td>{{ form_row(orderForm.email) }}</td>
                <td>{{ form_row(orderForm.pass) }}</td>
                <td>{{ form_row(orderForm.comm) }}</td>
            </tr>
            </tbody>
        </table>

      {{ form_end(orderForm) }}
        <br>
        <hr>


    </div>
{% endblock %}

{% block stylesheets %}
    <style>

    </style>
{% endblock %}